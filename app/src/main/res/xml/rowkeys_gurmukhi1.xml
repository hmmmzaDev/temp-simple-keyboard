<merge xmlns:latin="http://schemas.android.com/apk/res-auto">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked">
            <Key latin:keySpec="&#x0A59;" />
            <Key latin:keySpec="&#x0A73;" />
            <Key latin:keySpec="&#x0A0F;" />
            <Key latin:keySpec="&#x0A30;" />
            <Key latin:keySpec="&#x0A24;" />
            <Key latin:keySpec="&#x0A2F;" />
            <Key latin:keySpec="&#x0A09;" />
            <Key latin:keySpec="&#x0A17;" />
            <Key latin:keySpec="&#x0A18;" />
            <Key latin:keySpec="&#x0A2A;" />
        </case>
        <default>
            <Key latin:keySpec="&#x0A59;" />
            <Key latin:keySpec="&#x0A73;" />
            <Key latin:keySpec="&#x0A0F;" />
            <Key latin:keySpec="&#x0A30;" />
            <Key latin:keySpec="&#x0A24;" />
            <Key latin:keySpec="&#x0A2F;" />
            <Key latin:keySpec="&#x0A09;" />
            <Key latin:keySpec="&#x0A17;" />
            <Key latin:keySpec="&#x0A18;" />
            <Key latin:keySpec="&#x0A2A;" />
        </default>
    </switch>
</merge>
